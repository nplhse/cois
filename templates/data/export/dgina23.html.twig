        <a href="{{ path('app_export_index') }}"
            class="btn btn-outline-secondary">
            {{ 'Back to list'|trans }}
        </a>
    </div>

    <h1>
        {{ 'title.export.dgina23'|trans({}, 'app') }}
    </h1>

    <div class="card">
        <div class="card-header">
            {{ 'heading.export.dgina23.select'|trans({}, 'app') }}
        </div>
        <div class="card-body">
            <h4>
                {{ 'heading.export.dgina23.byUsage'|trans({}, 'app') }}
            </h4>
            <p>
                {{ 'text.export.dgina23.byUsage'|trans({}, 'app') }}
            </p>

            <table class="table table-striped">
                <thead>
                    <tr>
                        <th scope="col">
                            #
                        </th>
                        <th scope="col">
                            {{ 'table.export.name'|trans({}, 'app') }}
                        </th>
                        <th scope="col">
                            {{ 'table.export.content'|trans({}, 'app') }}
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <th scope="row">
                            1
                        </th>
                        <td>
                            total
                        </td>
                        <td>
                            {{
                                'table.export.dgina23.total_byUsage'|trans(
                                    {},
                                    'app'
                                )
                            }}
                        </td>
                    </tr>
                    <tr>
                        <th scope="row">
