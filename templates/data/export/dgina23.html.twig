{% extends 'data/_data_layout.html.twig' %}

{% block title %}
    {{ 'title.export'|trans({}, 'app') }} - {{ parent() }}
{% endblock %}

{% block breadcrumbs %}
    {{
        component(
            'breadcrumb',
            {
                items: [
                    {
                        url: url('app_export_index'),
                        label: 'title.export'|trans({}, 'app')
                    },
                    {
                        label: 'title.export.dgina23'|trans({}, 'app')
                    }
                ]
            }
        )
    }}
{% endblock %}

{% block content %}
