<div {{ attributes }}>
    {% if this.status == 'success' %}
        <i class="bi bi-check-circle-fill text-success">

        </i>
    {% elseif this.status == 'pending' %}
        <div class="spinner-border text-secondary spinner-border-sm"
             role="status">
            <span class="visually-hidden">
                {{ 'import.field.loading'|trans }}
            </span>
        </div>
    {% elseif this.status == 'incomplete' %}
        <i class="bi bi-exclamation-triangle-fill text-warning">

        </i>
    {% elseif this.status == 'failure' %}
        <i class="bi bi-exclamation-circle-fill text-danger">

        </i>
    {% elseif this.status == 'empty' %}
        <i class="bi bi-exclamation-circle-fill text-danger">

        </i>
    {% endif %} {{ this.status|trans }}
</div>
