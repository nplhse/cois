<div {{ attributes }}>
    <div class="card mb-3">
        <div class="card-header">
            {{ 'Import details'|trans }}
        </div>
        <div class="card-body">
            <h5 class="card-title">
                {{ 'Statistics'|trans }}
            </h5>
            <div class="row">
                <div class="col-6">
                    <h6 class="card-subtitle text-muted">
                        {{ 'Item count'|trans }}
                    </h6>
                    <p>
                        {{ import.rowCount }}
                    </p>
                </div>
                <div class="col-6">
                    <h6 class="card-subtitle text-muted">
                        {{ 'Runtime'|trans }}
                    </h6>
                    <p>
                        {{ import.runtime / 1000 }}s
                    </p>
                </div>
            </div>
            <div class="row">
                <div class="col-6">
                    <h6 class="card-subtitle text-muted">
                        {{ 'Times run'|trans }}
                    </h6>
                    <p>
                        {{ import.runCount }}
                    </p>
                </div>
                <div class="col-6">
                    <h6 class="card-subtitle text-muted">
                        {{ 'Skipped rows'|trans }}
                    </h6>
                    <p>
                        {{ import.skippedRows }}
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
