parameters:
    level: 6
    paths:
    	- src
    	- tests
    bootstrapFiles:
        - tests/bootstrap.php
        - vendor/bin/.phpunit/phpunit/vendor/autoload.php
    excludes_analyse:
        - %currentWorkingDirectory%/migrations/*
        - %currentWorkingDirectory%/vendor/*
    checkGenericClassInNonGenericObjectType: false
    checkMissingIterableValueType: false
